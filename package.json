{
  "name": "douzepoints",
  "version": "2.0.0",
  "description": "Eurovision scoreboard simulator",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:types-cli && yarn lint:last-commit",
    "lint:js": "eslint --ext .ts --ext .tsx .",
    "lint:css": "stylelint \"./src/styles.css\"",
    "lint:types-cli": "yarn tsc -p tsconfig.production.json",
    "lint:last-commit": "echo \"Info: no previous commits\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "generate-countries-json": "npx ts-node -P tsconfig.scripts.json scripts/generateCountriesJson.ts",
    "calculate-odds": "npx ts-node -P tsconfig.scripts.json scripts/calculateCountriesOdds.ts",
    "add-spokesperson-order": "npx ts-node -P tsconfig.scripts.json scripts/addSpokespersonOrder.ts",
    "convert-hex-to-hsl": "npx ts-node -P tsconfig.scripts.json scripts/convertHexToHsl.ts --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
