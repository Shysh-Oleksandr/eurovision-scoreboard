{
  "name": "douzepoints",
  "version": "2.0.0",
  "description": "Eurovision scoreboard simulator",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:css && vite build",
    "build:css": "tailwindcss -i ./src/styles.css -o ./src/output.css --minify",
    "tailwind": "tailwindcss -i ./src/styles.css -o ./src/output.css --watch",
    "preview": "vite preview",
    "preview:worker": "npm run build && wrangler dev",
    "deploy": "npm run build && wrangler deploy",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:types-cli && yarn lint:last-commit",
    "lint:js": "eslint --ext .ts --ext .tsx .",
    "lint:css": "stylelint \"./src/styles.css\"",
    "lint:types-cli": "yarn tsc -p tsconfig.production.json",
    "lint:last-commit": "echo \"Info: no previous commits\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "gsap": "^3.13.0",
    "idb": "^8.0.3",
    "react": "19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "19.1.0",
    "react-flip-toolkit": "^7.2.4",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.39",
    "prettier": "^2.8.8",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.4",
    "tw-colors": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^5.0.12"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
