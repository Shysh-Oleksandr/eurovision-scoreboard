    "idb": "^8.0.3",
    "next": "^16.0.0",
    "next-intl": "^4.5.3",
    "random-weighted-choice": "^0.1.4",
    "react": "19.1.0",
    "react-best-gradient-color-picker": "^3.0.14",
    "react-confetti": "^6.4.0",
